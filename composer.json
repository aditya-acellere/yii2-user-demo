{

    "name": "dektrium/yii2-user-demo",
    "description": "Yii2-user demo",
    "keywords": ["yii2", "yii2-user", "user management", "dektrium"],
    "type": "project",
    "license": "MIT",
    "homepage": "http://yii2-user.dmeroff.ru/",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "2.0.0",
        "yiisoft/yii2-swiftmailer": "2.0.0",
        "yiisoft/yii2-authclient": "2.0.0",
        "yiisoft/yii2-bootstrap": "2.0.0",
        "dektrium/yii2-user": "0.8.*@dev",
        "codeception/verify": "*",
        "codeception/specify": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "2.0.0",
        "yiisoft/yii2-debug": "2.0.0",
        "yiisoft/yii2-gii": "2.0.0"
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::setPermission",
            "yii\\composer\\Installer::generateCookieValidationKey"
        ]
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        },
        "writable": [
            "runtime",
            "web/assets"
        ],
        "executable": [
            "yii"
        ],
        "config": [
            "config/local/web.php"
        ]
    }
}
